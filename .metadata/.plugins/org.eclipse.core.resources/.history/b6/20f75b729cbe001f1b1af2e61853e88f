package models;

public class BillPayeeModel {
    private int payeeID;
    private String nic;
    private String billAccountNo;
    private String payeeCategory;
    private String serviceProvider;
    private String description;

    // Constructors
    public BillPayeeModel() {}

    public BillPayeeModel(int payeeID, String nic, String billAccountNo, String payeeCategory, String serviceProvider, String description) {
        this.payeeID = payeeID;
        this.nic = nic;
        this.billAccountNo = billAccountNo;
        this.payeeCategory = payeeCategory;
        this.serviceProvider = serviceProvider;
        this.description = description;
    }

    // Getters and Setters
    public int getPayeeID() {
        return payeeID;
    }

    public void setPayeeID(int payeeID) {
        this.payeeID = payeeID;
    }

    public String getNic() {
        return nic;
    }

    public void setNic(String nic) {
        this.nic = nic;
    }

    public String getBillAccountNo() {
        return billAccountNo;
    }

    public void setBillAccountNo(String billAccountNo) {
        this.billAccountNo = billAccountNo;
    }

    public String getPayeeCategory() {
        return payeeCategory;
    }

    public void setPayeeCategory(String payeeCategory) {
        this.payeeCategory = payeeCategory;
    }

    public String getServiceProvider() {
        return serviceProvider;
    }

    public void setServiceProvider(String serviceProvider) {
        this.serviceProvider = serviceProvider;
    }

    public String getDescription() {
        return description;
    }

    public void setDescription(String description) {
        this.description = description;
    }
}
